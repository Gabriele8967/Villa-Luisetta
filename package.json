{
  "name": "villa-luisetta",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:it": "ng build --localize --configuration=production",
    "build:en": "ng build --configuration=production --localize=en-US",
    "build:fr": "ng build --configuration=production --localize=fr-FR",
    "serve:it": "ng serve --configuration=development",
    "serve:en": "ng serve --configuration=development --localize=en-US",
    "serve:fr": "ng serve --configuration=development --localize=fr-FR",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:VillaLuisetta": "node dist/VillaLuisetta/server/server.mjs",
    "deploy": "gh-pages -d dist/VillaLuisetta"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/localize": "^20.1.7",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.2",
    "ajv": "^8.17.1",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.2",
    "@angular/build": "^20.0.2",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^20.0.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
